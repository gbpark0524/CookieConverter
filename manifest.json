{
  "name": "Cookie Converter",
  "manifest_version": 3,
  "version": "1.0.0",
  "permissions": ["cookies","storage"],
  "host_permissions": ["<all_urls>"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons8-cookie-64.png",
      "48": "colorCookie1.png",
      "128": "colorCookie1.png"
    }
  },
  "icons": {
    "16": "icons8-cookie-64.png",
    "48": "icons8-cookie-64.png",
    "128": "colorCookie1.png"
  },
  "options_ui": {
    "page": "options.html",
    "open_in_tab": true
  }
}